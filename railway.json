{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "DOCKERFILE",
    "dockerfilePath": "Dockerfile.django"
  },
  "deploy": {
    "healthcheckPath": "/up",
    "healthcheckTimeout": 100,
    "restartPolicyType": "ON_FAILURE",
    "startCommand": "/app/bin/docker-entrypoint-web supervisord -c /etc/supervisor/conf.d/supervisord.conf"
  }
}
